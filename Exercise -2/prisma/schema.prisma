generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Water_Connection {
  id                  Int     @id @default(autoincrement())
  water_connection_id Int?    @unique
  property_id         Int?    @unique
  description         String?
}
